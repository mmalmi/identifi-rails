<div class="jumbotron">
  <div class="row">
    <div class="col-lg-6">
      <div class="input-group">
        <input id="home-search-field" type="text" class="form-control" placeholder="">
        <span class="input-group-btn">
          <button id="home-search-button" class="btn btn-default" type="button"><span class="glyphicon glyphicon-search"></span></button>
        </span>
      </div><!-- /input-group -->
    <p class="help-block">Search by identifier (name, email, url, phone, ...)</p>
    </div><!-- /.col-lg-6 -->
  </div><!-- /.row -->
</div>

<div class="panel panel-default">
  <div class="panel-heading">
    <h3 class="panel-title">Latest feedback</h3>
  </div>
  <div class="panel-body">
    <table class="table">
      <%= render partial: "message", collection: @latest %>
    </table>
  </div>
</div>